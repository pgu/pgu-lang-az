<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <ui:style>
    .cell-ext {
        float: left;
        cursor: pointer;
        margin: 10px;
        margin-top: 5px;
    }
    .cell:active {
    }
    .cell:hover {
    }
    
    .cell-out {
    }
    .cell-med {
        border: 1px solid white;
    }
    .cell-in {
    }
    .cell-sub {
        position: relative;
        overflow: hidden;
    }
    .cell-text {
        position: absolute;
        color: white;
        font-size: 3.5em;
        line-height: 1;
        text-align: center;
    }
    
    </ui:style>
    
    <g:HTMLPanel ui:field="cellExt" addStyleNames="{style.cell-ext}">
        <g:HTMLPanel ui:field="cellOut" addStyleNames="{style.cell-out}">
            <g:HTMLPanel ui:field="cellMed" addStyleNames="{style.cell-med}">
                <g:HTMLPanel ui:field="cellIn" addStyleNames="{style.cell-in}">
                    <g:HTMLPanel ui:field="cellSub" addStyleNames="{style.cell-sub}">
                        <g:HTML ui:field="cellText" addStyleNames="{style.cell-text}"/>
                    </g:HTMLPanel>
                </g:HTMLPanel>
            </g:HTMLPanel>
        </g:HTMLPanel>
    </g:HTMLPanel>
    
</ui:UiBinder> 