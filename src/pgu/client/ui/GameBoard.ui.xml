<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:style>
        .card {
            cursor: pointer;
            margin: 3px;
            
            position: relative;
            float: left;
            
            box-shadow: 0 3px 5px #003805;
            -moz-box-shadow: 0 3px 5px #003805;
            -ms-box-shadow: 0 3px 5px #003805;
            -o-box-shadow: 0 3px 5px #003805;
            -webkit-box-shadow: 0 3px 5px #003805;
            
            border-top: 1px solid white;
            background-color: #DBFFE0;
            
            border-radius: 3px;
            -moz-border-radius: 3px; 
            -webkit-border-radius: 3px; 
        }
        .board {
            margin: 0px;
            padding: 0px;
            
            background-color: #115F18;
            
            border-radius: 3px;
            -moz-border-radius: 3px; 
            -webkit-border-radius: 3px; 
        }
    </ui:style>
    
    <g:HTMLPanel ui:field="board" styleName="{style.board}">
        <g:HTML ui:field="card01" styleName="{style.card}"></g:HTML>
